{
  "version": 2,
  "buildCommand": "",
  "outputDirectory": ".",
  "routes": [
    {
      "src": "/(.*)",
      "headers": {
        "Cache-Control": "no-store, max-age=0",
        "Access-Control-Allow-Origin": "*"
      }
    },
    {
      "src": "/api/(.*)",
      "dest": "https://clever-it-hazel.vercel.app/api/$1"
    },
    {
      "src": "/social/(.*)",
      "dest": "https://claverit-social.vercel.app/api/$1"
    },
    {
      "src": "/chat/(.*)",
      "dest": "https://claverit-chat-backend.onrender.com/$1"
    },
    {
      "src": "/notify/(.*)",
      "dest": "https://claverit-notifications.vercel.app/api/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
